{
  "cli": {
    "version": ">= 16.32.0",
    "appVersionSource": "remote"
  },
  "build": {
    "dev": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "dev"
      }
    },
    "sit": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "sit"
      }
    },
    "prod": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "prod"
      }
    }
  },
  "submit": {
    "production": {}
  }
}