{
  "cli": {
    "version": ">= 16.32.0",
    "appVersionSource": "remote"
  },
  "build": {
    "dev": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "versionCode": "auto"
      },
      "env": {
        "APP_ENV": "dev"
      },
      "autoIncrement": true,
      "channel": "dev"
    },
    "sit": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "versionCode": "auto"
      },
      "env": {
        "APP_ENV": "sit"
      },
      "autoIncrement": true,
      "channel": "sit"
    },
    "prod": {
      "android": {
        "buildType": "app-bundle",
        "versionCode": "auto"
      },
      "env": {
        "APP_ENV": "prod"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}